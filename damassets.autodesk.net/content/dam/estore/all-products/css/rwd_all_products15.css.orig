/* color color*/
.body-content {
  background-color: #f1f1f1 !important; }

.body-content a:visited {
  color: #444 !important; }

.body-content.wp-search {
  background-color: #fff !important;
  border-bottom: 1px solid #ccc; }

.body-content.glossaire {
  background-color: #fff !important;
  border-top: 1px solid #ccc; }

.wp-all-products a {
  border-bottom: none !important; }

.wp-all-products h1 {
  color: #0696D7;
  font-size: 2.8rem;
  margin: 1.1em 0 0 0; }

.wp-all-products h2 {
  color: #444;
  font-size: 1.9rem;
  font-weight: 400;
  margin: 0.813em 0 0.625em 0; }

.wp-all-products .hidden-filters {
  display: none; }

.wp-all-products .ban {
  background-image: url(//static-ww.autodesk.net/content/dam/estore/all-products/banners/estore_all_products_banner.jpg);
  min-height: 178px;
  border-bottom: 1px solid #ccc;
  background-repeat: no-repeat;
  background-position: center center;
  background-color: #f5f5f5; }

.wp-all-products .ban h1 {
  margin: 0 !important;
  padding: 0 !important;
  font-size: 3.1rem;
  line-height: 4.4;
  color: #444;
  text-transform: uppercase; }

.wp-all-products .home {
  margin-bottom: 2.688em; }

.wp-all-products .best::after {
  content: " ";
  display: block;
  clear: both; }

.wp-all-products .view2 {
  display: none; }

.wp-all-products .wp-search h3 {
  text-transform: uppercase !important; }

.wp-all-products .wp-search .search h3 {
  line-height: 2.8em;
  margin: 0;
  padding: 0; }

.wp-all-products .wp-search .filter h3 {
  line-height: 2.0em;
  margin: 0;
  padding: 0; }

.wp-all-products ::-webkit-input-placeholder {
  font-size: 1.188em;
  color: #888;
  line-height: 1.188; }

.wp-all-products :-moz-placeholder {
  font-size: 1.188em;
  color: #888;
  line-height: 2.5em; }

.wp-all-products ::-moz-placeholder {
  font-size: 1.188em;
  color: #888;
  line-height: 2.5em; }

.wp-all-products :-ms-input-placeholder {
  font-size: 1.188em;
  color: #888;
  line-height: 1.188; }

.wp-all-products ::-ms-value {
  background: none;
  color: #888; }

.wp-all-products *:focus {
  outline: none; }

.wp-all-products .wp-search {
  padding: 10px 0; }

.wp-all-products input#search {
  width: 100%;
  box-sizing: border-box !important;
  height: 3.125em;
  color: #0696D7;
  font-size: 1.6rem;
  font-weight: bold;
  text-indent: 10px;
  border: 1px solid #ccc;
  -webkit-appearance: none; }

.wp-all-products .icon-select {
  position: absolute;
  width: 20px;
  height: 20px;
  right: 10px;
  top: 10px;
  fill: #aaa;
  z-index: 0; }

.wp-all-products select {
  background: #fff;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  z-index: 0;
  position: relative;
  -webkit-border-radius: 0px;
  font-weight: bold;
  height: 2.02em;
  padding-left: 15px;
  background-color: transparent;
  border: solid 1px #ccc;
  color: #888;
  cursor: pointer; }

.wp-all-products ::-ms-expand {
  display: none; }

.wp-all-products ::-ms-clear {
  width: 0;
  height: 0; }

.wp-all-products ::-ms-reveal {
  width: 0;
  height: 0; }

.wp-all-products .search ul {
  list-style: none;
  margin: 0;
  padding: 0;
  background-color: #fff; }

.wp-all-products ul li ul.drop {
  display: none; }

.wp-all-products ul li:hover ul.drop {
  display: block; }

.wp-all-products .border {
  border: solid 1px #ccc;
  border-top: none; }

.wp-all-products ul.drop {
  margin: 0;
  padding: 0;
  list-style: none;
  position: absolute;
  z-index: 1;
  width: 100%; }

.wp-all-products ul.drop li {
  margin: 0 15px;
  padding: 15px 0 12px 10px;
  border-bottom: 1px dotted #ccc; }

.wp-all-products ul.drop li:hover {
  background-color: #f4f4f4; }

.wp-all-products ul.drop li:last-child {
  border-bottom: none; }

.wp-all-products ul.drop li a {
  display: block;
  color: #444;
  font-weight: normal; }

.wp-all-products ul.drop li:hover a {
  color: #0696D7;
  font-weight: bold; }

.wp-all-products .notFound {
  display: none; }

.wp-all-products h3 {
  color: #545454;
  font-size: 1.188em; }

.wp-all-products span.taxonomy, .wp-all-products span.raz {
  position: relative;
  display: inline-block;
  border: solid #ccc 1px;
  font-size: 0.875em;
  width: 8.625em;
  height: 2.313em;
  line-height: 2.313;
  margin: 0 0 0 8px;
  color: #888;
  text-indent: 35px;
  cursor: pointer;
  float: right;
  background-repeat: no-repeat;
  background-position: left center;
  margin-bottom: 10px;
  font-weight: 700;
  text-transform: uppercase; }

.wp-all-products span.taxonomy.select {
  background-color: #0696D7;
  color: #fff; }

.wp-all-products .apple {
  width: 20px;
  height: 20px;
  fill: #aaa;
  position: absolute;
  top: 4px;
  left: 7px; }

.wp-all-products .windows {
  width: 20px;
  height: 20px;
  fill: #aaa;
  position: absolute;
  top: 4px;
  left: 7px; }

.wp-all-products .reset {
  width: 15px;
  height: 15px;
  fill: #777;
  position: absolute;
  top: 7px;
  left: 7px; }

.wp-all-products [data="reset"] {
  background-color: #eee; }

.wp-all-products .wrapLoupe {
  position: absolute;
  top: 2px;
  right: 2px;
  height: 45px;
  width: 50px;
  cursor: pointer;
  background-color: #fff; }

.wp-all-products .loupe {
  width: 25px;
  height: 25px;
  fill: #777;
  position: relative;
  top: 10px;
  left: 10px; }

.wp-all-products .wp-product-box.std .marg {
  margin: 0 0.5em 0.5em 0; }

.wp-all-products .wp-product-box.std a.wp-product {
  background-color: #fff;
  display: block;
  height: 5.000em;
  -webkit-box-shadow: 2px 2px 0px #dddddd;
  -moz-box-shadow: 2px 2px 0px #dddddd;
  box-shadow: 2px 2px 0px #dddddd; }
  .wp-all-products .wp-product-box.std a.wp-product:hover {
    background-color: #f2fafd; }
  .wp-all-products .wp-product-box.std a.wp-product .bdg {
    border-right: 1px solid #f1f1f1;
    display: table-cell;
    vertical-align: middle;
    text-align: center;
    width: 5.000em;
    height: 5.000em; }
    .wp-all-products .wp-product-box.std a.wp-product .bdg img {
      max-width: 3.750em;
      max-height: 3.750em;
      display: table-cell;
      margin: auto; }
  .wp-all-products .wp-product-box.std a.wp-product .cnt {
    margin-left: 0.750em;
    display: table-cell;
    vertical-align: middle;
    text-align: left;
    width: 12.125em;
    height: 5.000em;
    padding-left: 0.750em; }
    .wp-all-products .wp-product-box.std a.wp-product .cnt p {
      font-size: 16px;
      font-size: 1.6rem !important;
      color: #666 !important; }

.wp-all-products .wp-product-box.std .wp-arrow {
  display: none; }

.wp-all-products .wp-product-box.others_ .marg {
  margin: 0 0.063em 0.063em 0; }

.wp-all-products .wp-product-box.others_ a.wp-product {
  background-color: #fff;
  display: block;
  height: 3.625em; }
  .wp-all-products .wp-product-box.others_ a.wp-product:hover {
    background-color: #f2fafd; }
  .wp-all-products .wp-product-box.others_ a.wp-product .bdg {
    display: none; }
  .wp-all-products .wp-product-box.others_ a.wp-product .cnt {
    margin-left: 0.750em;
    display: table-cell;
    vertical-align: middle;
    text-align: left;
    width: 80%;
    height: 3.625em;
    padding: 0 1.2em 0 1em; }
    .wp-all-products .wp-product-box.others_ a.wp-product .cnt p {
      font-size: 16px;
      font-size: 1.6rem !important;
      color: #666 !important; }

.wp-all-products .wp-product-box.others_ .wp-arrow {
  width: 1.00em;
  height: 1.00em;
  position: absolute;
  right: 10px;
  top: 34%;
  fill: #aaa; }

.wp-all-products .wp-product-box {
  position: relative;
  z-index: 0; }
  .wp-all-products .wp-product-box span.promo {
    color: #0696D7;
    font-size: 1.3rem; }

.wp-all-products .expandLine {
  background-color: #fff;
  cursor: pointer;
  min-height: 2.500em;
  line-height: 2.500;
  color: #777;
  text-transform: uppercase;
  position: relative; }
  .wp-all-products .expandLine:hover {
    background-color: #f2fafd; }

.wp-all-products .expandLine span {
  font-size: 1.4rem;
  font-weight: 700; }

.wp-all-products .expandLine span.plusminus {
  color: #0696D7;
  font-size: 2.0rem;
  line-height: 2.0;
  margin: 0 0.5em; }

.wp-all-products .expandLine .line {
  height: 1px;
  width: 100%;
  background: #ccc;
  position: relative;
  bottom: -2.500em; }

.wp-all-products .glossaire a {
  display: block;
  color: #666;
  font-size: 1.4rem !important;
  line-height: 38px;
  font-weight: 100;
  border-bottom: 1px solid #DADADA !important;
  min-height: 38px; }
  .wp-all-products .glossaire a:hover {
    color: #0696D7; }

.wp-all-products .glossaire h2 {
  color: #444;
  font-size: 1.9rem;
  font-weight: 100;
  display: block;
  line-height: 38px;
  min-height: 38px;
  border-bottom: 1px solid #DADADA; }

/* Large screens */
@media only screen and (min-width: 64.063em) {
  .wp-product-box {
    position: relative;
    z-index: 0; }
    .wp-product-box .over {
      position: absolute;
      top: 1px;
      z-index: 0;
      height: 3px;
      width: 0;
      background-color: #0696D7; }
    .wp-product-box:hover .over {
      width: calc(100% - 0.500em);
      margin: 0 0.063em 0 0; }
  .wp-product-box.others_ {
    position: relative;
    z-index: 0; }
    .wp-product-box.others_:hover .over {
      width: 100%;
      margin: 0 0.063em 0 0; }
  .glossaire .responsive-column-1 a:last-child,
  .glossaire .responsive-column-2 a:last-child,
  .glossaire .responsive-column-3 a:last-child {
    border-bottom: none !important; } }
